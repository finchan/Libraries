<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!--Runtime -
Outbound Handler Sequence - Logical Handler > Soap Handler
Inbound Handler Sequence - SOAP Handler > Logical Handler
-->
<javaee:handler-chains
        xmlns:javaee="http://java.sun.com/xml/ns/javaee"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <javaee:handler-chain>
        <javaee:handler>
            <javaee:handler-class>com.xavier.webservices.upandrunning.ch03.fibsoap12.UUIDValidator</javaee:handler-class>
        </javaee:handler>
    </javaee:handler-chain>
</javaee:handler-chains>